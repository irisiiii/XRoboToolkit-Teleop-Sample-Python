<?xml version="1.0" encoding="utf-8"?>
<!-- 
  Fixed URDF for dual_arm robot
  Modified to use direct axis definitions (X7S style) instead of rpy transformations
  
  Key changes:
  - Converted all rpy-transformed Z-axis rotations to direct axis vectors
  - Simplified origin rpy values where possible
  - Maintained exact kinematic chain as original
  
  Conversion logic:
  - rpy="1.5708 0 0" (90° around X) + axis="0 0 1" → axis="0 1 0" (Y-axis)
  - rpy="-1.5708 0 0" (-90° around X) + axis="0 0 1" → axis="0 -1 0" (-Y-axis)
  - rpy="-1.5708 0 -3.1416" (-90° around X, -180° around Z) + axis="0 0 1" → axis="0 1 0" (Y-axis)
  - rpy="1.5708 0 -3.1416" (90° around X, -180° around Z) + axis="0 -1 0" (-Y-axis)
-->
